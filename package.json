{
  "name": "webpack-assets-handling",
  "version": "2.0.0",
  "description": "Webpack styles hadling test project",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all webpack-dev-derver",
    "webpack": "webpack --config=./webpack/webpack.dev.config",
    "webpack-dev-derver": "webpack-dev-server --config=./webpack/webpack.dev.config --hot --progress --colors --open",
    "build-dev": "npm-run-all webpack",
    "build-prod": "webpack --config=./webpack/webpack.production.config --progress --profile --colors",
    "build-dll": "webpack --config=./webpack/webpack.config.dll --progress --profile --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shimizacken/webpack-assets-handling.git"
  },
  "author": "Shimi Zacken",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shimizacken/webpack-assets-handling/issues"
  },
  "homepage": "https://github.com/shimizacken/webpack-assets-handling#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.2",
    "react-hot-loader": "^4.0.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.19.1",
    "webpack": "^2.7.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "js-polyfills": "^0.1.34",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "react-svg-loader": "^2.1.0"
  }
}
